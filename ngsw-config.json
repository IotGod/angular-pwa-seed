{
  "index": "/index.html",

  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js",
        "/*.woff2",
        "/*.ttf",
        "/assets/icon.png",
        "/assets/logo.png",
        "/svg/**",
        "/scripts/**"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],

  "dataGroups": [{
    "name": "api-freshness",
    "urls": [
      "/api"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 100,
      "maxAge": "3d",
      "timeout": "10s"
    }
  }
]
}
